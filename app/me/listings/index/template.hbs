<ul class="list-group">
  <li class="list-group-item list-group-item-info">
    My Listings
  </li>
  {{#each model as |listing|}}
    <h5>{{listing.title}}</h5>
    <p>{{listing.description}}</p>
    <p>Price: {{listing.price}}</p>
  {{else}}
    <li class="list-group-item">
      <h4>You don't have any listings yet!</h4>
      Add Listing
    </li>
  {{/each}}
</ul>