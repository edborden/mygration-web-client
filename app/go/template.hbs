{{#each uniqueLocationCommonNames as |name|}}
  <ul class="list-group">
    <li class="list-group-item">
      <div class="row">
        <div class="col m12">
          <h4>{{name}}</h4>
        </div>
        <div class="col m4">
          {{location-listings countryName=name allProfiles=model}}
        </div>
      </div>
    </li>
  </ul>
{{else}}
  <h4>No listings!</h4>
{{/each}}